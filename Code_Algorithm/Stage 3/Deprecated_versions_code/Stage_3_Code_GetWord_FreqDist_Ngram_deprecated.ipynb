    "def get_dict_key_words(Target_dir, delimiter = 'TopWords'):\n",
    "    \n",
    "    # Change directory to your target dir\n",
    "    os.chdir(Target_dir)\n",
    "    # Define the list of files you want to obtain using the delimiter. \n",
    "    List_KeyWordFiles = [x for x in os.listdir() if delimiter in x]\n",
    "    \n",
    "    # Dictionary:  Key = File Name, Value = Key Words\n",
    "    Dict = {}\n",
    "    \n",
    "    # Iterate over list of key word files\n",
    "    for file in List_KeyWordFiles:\n",
    "        # Read into memory the file\n",
    "        df = pd.read_excel(file)\n",
    "        # Iterate over each column of the dataframe\n",
    "        for col in df.columns:\n",
    "            # Limit the results to only strings (no none values)\n",
    "            Dict[file] = set([word for word in df[col] if isinstance(word, str)])\n",
    "    # Return Dictionary of Key Words to user. \n",
    "    return Dict    "
   ]
